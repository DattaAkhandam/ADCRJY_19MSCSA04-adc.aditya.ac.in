import java.util.Scanner;
import java.util.Stack;
public class Demo
{
static Stack<Integer> st=new Stack<Integer>();
public static void main(String[] args)
{
Scanner sc=new Scanner(System.in);
int n=sc.nextInt();
int a[]=new int[n];
for(int i=0;i<n;i++)
a[i]=sc.nextInt();
insert(a,n);
display();
}
public static void insert(int a[],int n)
{
for(int i=0;i<n;i++)
st.push(a[i]);
}
{
if(a[i]%2==1)
st.remove(a[i]);
{
st.display();
for(int i:st)
System.out.print(i+"");
}
}
}

Output:
5
10 20 30 40 50
20 40
