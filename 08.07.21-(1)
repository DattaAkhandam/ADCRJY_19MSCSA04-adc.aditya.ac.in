import java.util.*;
public class first
{
public static void main (String[] args)
{
Scanner  s=new Scanner(System.in);
int r=s.nextInt();
int c=s.nextInt();
int arr[ ] [ ] =new int [r][c];
for(int i=0;i<r;i++)
for(int j=0;j<c;j++)
arr [ i ][ j ]=s.nextInt();
int sum=0;
for(int j=0;j<c;j++)
{
for(int i=0;i<r;i++)
{
sum+=arr[ i ] [ j ];
}
System.out.println(" Col "+(j+1)+" Sum : " +sum);
sum=0;
}
}
}


Output:
2
2
10 20
30 40
Col 1 Sum : 40
Col 2 Sum : 60
