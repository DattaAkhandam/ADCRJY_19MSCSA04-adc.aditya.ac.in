import java.util.*;
class Solution
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
System.out.println("Enter size of the array");
int n=sc.nextInt();
int arr[]=new int[n];
System.out.println(" Enter values into array ");
for(int i=0;i<n;i++)
arr[i]=sc.nextInt();
int max1,max2;
for(int i=0;i<n;i++)
{
if(arr[i]>max1)
{
max2=max1;
max1=arr[i];
}
else if(arr[i]>max2)
{
max2=arr[i];
}
}
System.out.print("Second largest number in array is :" + max2);
}
}



Output:
Enter size of the array
5
Enter values into array
10 42 6 98 73
Second largest number in array is : 73
