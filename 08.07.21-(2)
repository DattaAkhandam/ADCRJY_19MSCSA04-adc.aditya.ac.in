import java.util.*;
public class first
{
public static void main (String[] args)
{
int arr [ ] [ ] =new int [ ] [ ] { { 9,5,65 },{ 14,81,32 },{ 23,3,21 }};
int l;
for(int j=0;j<3;j++)
{
l=arr[0][j];
for(int i=1;i<3;i++)
{
if(l<arr [ i ][ j ])
l=arr [ i ][ j ];
}
System.out.println(" Largest element in "+(j+1)+"col : " +l);
}
}
}



Output:
Largest in 1 col : 23
Largest in 2 col : 81
Largest in 3 col : 65
